generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Artist {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique 
  artwork Artwork[]
}

model Artwork {
  id       Int      @id @default(autoincrement())
  title    String
  content  String?
  artistId Int
  artist   Artist   @relation(fields: [artistId], references: [id])
}
